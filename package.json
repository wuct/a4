    "webpack": "^1.13.3"
  },
  "dependencies": {
    "d3-shape": "^1.0.4",
    "date-fp": "^4.4.2",
    "ramda": "^0.22.1",
    "recompose": "^0.20.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  }
