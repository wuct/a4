    "build": "babel modules --out-dir lib --ignore __tests__",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wuct/a4.git"
  },
  "keywords": [
    "react",
    "chart",
    "graph",
    "plot"
  ],
  "files": [
    "lib"
  ],
  "author": "wuct",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wuct/a4/issues"
  },
  "homepage": "https://github.com/wuct/a4#readme",
  "ava": {
    "files": [
      "**/test/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.4.0",
    "react": "^15.3.0",
